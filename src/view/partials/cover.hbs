<nav class="navbar navbar-expand-sm navbar-light">
    <div class="container-fluid">
        <a class="navbar-brand" href="#summary-main">{{cover.first-name}} <span id="last-name">{{cover.last-name}}</span> <span id="site-name">{{cover.site-name}}</span></a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar-collapse-menu">
            <div id="burger">
                <div class="bar top"></div>
                <div class="bar bottom"></div>
            </div>
        </button>
        <div class="collapse navbar-collapse" id="navbar-collapse-menu">
            <ul class="navbar-nav ml-auto">
                {{#each cover.links}}
                <li class="nav-item delay-{{@index}}">
                    <a href="{{this.href}}" class="nav-link" data-toggle="collapse" data-target="#navbar-collapse-menu">{{this.text}}</a>
                </li>
                {{/each}}
            </ul>
        </div>
    </div>
</nav>